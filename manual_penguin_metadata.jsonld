{
  "@context": [
    "http://www.w3.org/ns/csvw",
    "https://w3id.org/csvw-safe-context.jsonld"
  ],
  "@type": "csvw:Table",
  "name": "penguins",

  "csvw-safe:public.privacyUnit": "penguin_id",

  "csvw-safe:bounds.maxContributions": 2,
  "csvw-safe:bounds.maxLength": 344,
  "csvw-safe:public.length": 344,

  "csvw:tableSchema": {
    "columns": [

      {
        "@type": "csvw:Column",
        "name": "penguin_id",
        "datatype": "integer",
        "required": true,
        "csvw-safe:public.privacyId": true
      },

      {
        "@type": "csvw:Column",
        "name": "species",
        "datatype": "string",
        "required": true,

        "csvw-safe:bounds.maxGroupsPerUnit": 1,
        "csvw-safe:bounds.maxNumPartitions": 3,
        "csvw-safe:public.exhaustivePartitions": true,

        "csvw-safe:public.partitions": [
          {
            "@type": "csvw-safe:Partition",
            "csvw-safe:predicate": { "partitionValue": "Adelie" },
            "csvw-safe:bounds.maxContributions": 1,
            "csvw-safe:bounds.maxLength": 200
          },
          {
            "@type": "csvw-safe:Partition",
            "csvw-safe:predicate": { "partitionValue": "Chinstrap" },
            "csvw-safe:bounds.maxContributions": 1,
            "csvw-safe:bounds.maxLength": 200
          },
          {
            "@type": "csvw-safe:Partition",
            "csvw-safe:predicate": { "partitionValue": "Gentoo" },
            "csvw-safe:bounds.maxContributions": 1,
            "csvw-safe:bounds.maxLength": 200
          }
        ]
      },

      {
        "@type": "csvw:Column",
        "name": "island",
        "datatype": "string",
        "required": true,

        "csvw-safe:bounds.maxGroupsPerUnit": 1,
        "csvw-safe:bounds.maxNumPartitions": 3,
        "csvw-safe:public.exhaustivePartitions": true,

        "csvw-safe:public.partitions": [
          { "@type": "csvw-safe:Partition", "csvw-safe:predicate": { "partitionValue": "Torgersen" } },
          { "@type": "csvw-safe:Partition", "csvw-safe:predicate": { "partitionValue": "Biscoe" } },
          { "@type": "csvw-safe:Partition", "csvw-safe:predicate": { "partitionValue": "Dream" } }
        ]
      },

      {
        "@type": "csvw:Column",
        "name": "bill_length_mm",
        "datatype": "double",
        "required": true,
        "minimum": 30,
        "maximum": 65
      },

      {
        "@type": "csvw:Column",
        "name": "bill_depth_mm",
        "datatype": "double",
        "required": false,
        "minimum": 13,
        "maximum": 23,
        "csvw-safe:synth.nullableProportion": 0.05
      },

      {
        "@type": "csvw:Column",
        "name": "flipper_length_mm",
        "datatype": "double",
        "required": false,
        "minimum": 150,
        "maximum": 250,
        "csvw-safe:synth.nullableProportion": 0.05,

        "csvw-safe:bounds.maxGroupsPerUnit": 2,
        "csvw-safe:bounds.maxNumPartitions": 2,

        "csvw-safe:public.partitions": [
          {
            "@type": "csvw-safe:Partition",
            "csvw-safe:predicate": { "lowerBound": 150, "upperBound": 200 }
          },
          {
            "@type": "csvw-safe:Partition",
            "csvw-safe:predicate": { "lowerBound": 200, "upperBound": 250 }
          }
        ]
      },

      {
        "@type": "csvw:Column",
        "name": "body_mass_g",
        "datatype": "double",
        "required": false,
        "minimum": 2000,
        "maximum": 7000,
        "csvw-safe:synth.nullableProportion": 0.05
      },

      {
        "@type": "csvw:Column",
        "name": "sex",
        "datatype": "string",
        "required": false,
        "csvw-safe:synth.nullableProportion": 0.2,

        "csvw-safe:bounds.maxGroupsPerUnit": 1,
        "csvw-safe:bounds.maxNumPartitions": 2,

        "csvw-safe:public.partitions": [
          { "@type": "csvw-safe:Partition", "csvw-safe:predicate": { "partitionValue": "MALE" } },
          { "@type": "csvw-safe:Partition", "csvw-safe:predicate": { "partitionValue": "FEMALE" } }
        ]
      }
    ]
  },

  "csvw-safe:additionalInformation": [
    {
      "@type": "csvw-safe:ColumnGroup",
      "csvw-safe:columns": ["sex", "island"],

      "csvw-safe:bounds.maxGroupsPerUnit": 1,
      "csvw-safe:bounds.maxNumPartitions": 6,

      "csvw-safe:public.partitions": [
        {
          "@type": "csvw-safe:Partition",
          "csvw-safe:predicate": {
            "components": {
              "sex": { "partitionValue": "MALE" },
              "island": { "partitionValue": "Torgersen" }
            }
          }
        },
        {
          "@type": "csvw-safe:Partition",
          "csvw-safe:predicate": {
            "components": {
              "sex": { "partitionValue": "FEMALE" },
              "island": { "partitionValue": "Torgersen" }
            }
          }
        }
      ]
    }
  ]
}
