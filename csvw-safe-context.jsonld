{
  "@context": {
    "@version": 1.1,

    "csvw": "http://www.w3.org/ns/csvw#",
    "dp": "https://w3id.org/csvw-dp#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    /* =====================================================
       Core CSVW terms
       ===================================================== */

    "Table": "csvw:Table",
    "TableSchema": "csvw:TableSchema",
    "Column": "csvw:Column",

    "tableSchema": {
      "@id": "csvw:tableSchema",
      "@type": "@id"
    },

    "columns": {
      "@id": "csvw:column",
      "@type": "@id",
      "@container": "@list"
    },

    "name": "csvw:name",
    "datatype": {
      "@id": "csvw:datatype",
      "@type": "@id"
    },

    /* =====================================================
       DP core classes
       ===================================================== */

    "DPBounded": "dp:DPBounded",
    "GroupingKey": "dp:GroupingKey",
    "ColumnGroup": "dp:ColumnGroup",
    "PartitionKey": "dp:PartitionKey",

    /* =====================================================
       DP bounds (generic, reusable)
       ===================================================== */

    "maxPartitionLength": {
      "@id": "dp:maxPartitionLength",
      "@type": "xsd:positiveInteger"
    },

    "partitionLength": {
      "@id": "dp:partitionLength",
      "@type": "xsd:positiveInteger"
    },

    "maxPartitionContribution": {
      "@id": "dp:maxPartitionContribution",
      "@type": "xsd:positiveInteger"
    },

    "maxInfluencedPartitions": {
      "@id": "dp:maxInfluencedPartitions",
      "@type": "xsd:positiveInteger"
    },

    "maxNumPartitions": {
      "@id": "dp:maxNumPartitions",
      "@type": "xsd:positiveInteger"
    },

    /* =====================================================
       Column-level DP metadata
       ===================================================== */

    "privacyId": {
      "@id": "dp:privacyId",
      "@type": "xsd:boolean"
    },

    "nullableProportion": {
      "@id": "dp:nullableProportion",
      "@type": "xsd:decimal"
    },

    /* =====================================================
       Grouping & partitions
       ===================================================== */

    "columnGroup": {
      "@id": "dp:ColumnGroup",
      "@type": "@id"
    },

    "columnsInGroup": {
      "@id": "dp:columns",
      "@type": "@id",
      "@container": "@list"
    },

    "publicPartitions": {
      "@id": "dp:publicPartitions",
      "@container": "@list"
    },

    /* =====================================================
       PartitionKey structure
       ===================================================== */

    "partitionValue": "dp:partitionValue",

    "lowerBound": {
      "@id": "dp:lowerBound",
      "@type": "xsd:decimal"
    },

    "upperBound": {
      "@id": "dp:upperBound",
      "@type": "xsd:decimal"
    },

    "partitionLabel": "dp:partitionLabel",

    "components": {
      "@id": "dp:components",
      "@type": "@id"
    }
  }
}
